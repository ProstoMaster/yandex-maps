{
  "assignees": [
    "daniluk4000"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "vue",
        "nuxt"
      ],
      matchCurrentVersion: "<=2",
      "allowedVersions": "<=2"
    },
    {
      "matchPackageNames": [
        "vue"
      ],
      matchCurrentVersion: "3.0.0",
      "allowedVersions": "3.0.0"
    },
    {
      matchDepTypes: [
        "peerDependencies",
        "engines"
      ],
      enabled: false
    }
  ],
  "labels": [
    "2.x",
    "dependencies"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "updateInternalDeps": true,
  "baseBranches": [
    "master"
  ],
  extends: [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    "group:monorepos",
    "group:recommended",
    "replacements:all",
    "workarounds:all",
    "group:allNonMajor"
  ],
  rangeStrategy: "bump",
  "npm": {
    "commitMessageTopic": "{{prettyDepType}} {{depName}}"
  }
}
