{
  "assignees": [
    "daniluk4000"
  ],
  rebaseWhen: "behind-base-branch",
  "packageRules": [
    {
      "matchPackageNames": [
        "vue",
        "nuxt"
      ],
      matchCurrentVersion: "<=2",
      "allowedVersions": "<=2"
    },
    {
      matchDepTypes: [
        "peerDependencies",
        "engines"
      ],
      enabled: false
    },
    {
      "matchUpdateTypes": ["minor", "patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ],
  "labels": [
    "2.x",
    "dependencies"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    automerge: true
  },
  "updateInternalDeps": true,
  "baseBranches": [
    "master"
  ],
  extends: [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    "group:monorepos",
    "group:recommended",
    "replacements:all",
    "workarounds:all",
    "group:allNonMajor"
  ],
  rangeStrategy: "bump",
  "npm": {
    "commitMessageTopic": "{{prettyDepType}} {{depName}}"
  }
}
