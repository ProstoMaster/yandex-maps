{
  "assignees": [
    "daniluk4000"
  ],
  rebaseWhen: "behind-base-branch",
  "github-actions": {
    automerge: true
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "@yandex/ymaps3-types"
      ],
      automerge: false,
      groupName: "Yandex",
    },
    {
      "matchPackageNames": [
        "vue",
        "nuxt"
      ],
      automerge: true,
      matchCurrentVersion: "<=2",
      "allowedVersions": "<=2"
    },
    {
      matchDepTypes: [
        "peerDependencies",
        "engines"
      ],
      enabled: false
    },
    {
      "groupName": "all non-major dependencies",
      "groupSlug": "all-minor-patch",
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ],
  "labels": [
    "2.x",
    "dependencies"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    automerge: true
  },
  "updateInternalDeps": true,
  "baseBranches": [
    "master"
  ],
  extends: [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    "group:monorepos",
    "group:recommended",
    "replacements:all",
    "workarounds:all"
  ],
  rangeStrategy: "replace",
  "npm": {
    "commitMessageTopic": "{{prettyDepType}} {{depName}}"
  }
}
