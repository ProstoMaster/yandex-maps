{
  "assignees": [
    "daniluk4000"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "vue",
        "nuxt"
      ],
      "allowedVersions": "<3"
    },
    {
      matchDepTypes: [
        "peerDependencies",
        "engines"
      ],
      enabled: false
    }
  ],
  "labels": [
    "2.x",
    "dependencies"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "updateInternalDeps": true,
  "baseBranches": [
    "v2"
  ],
  extends: [
    "config:base",
    "group:allNonMajor"
  ],
  rangeStrategy: "bump",
  "npm": {
    "commitMessageTopic": "{{prettyDepType}} {{depName}}"
  }
}
