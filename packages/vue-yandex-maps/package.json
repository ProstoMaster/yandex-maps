{
  "name": "vue-yandex-maps",
  "version": "2.0.0-beta.1",
  "description": "Yandex Maps component for VueJS.",
  "main": "./dist/vue-yandex-maps.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/vue-yandex-maps.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt/types.d.ts",
      "import": "./dist/nuxt/module.mjs",
      "require": "./dist/nuxt/module.cjs"
    },
    "./nuxt2": "./dist/nuxt2.js",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "build": "yarn workspace nuxt prepack && yarn build:package-only",
    "build:package-only": "vue-tsc --noEmit && vite build",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "test": "jest",
    "npm:publish": "npm publish --tag v2"
  },
  "files": [
    "package.json",
    "dist"
  ],
  "engines": {
    "node": ">=16.14"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/PNKBizz/vue-yandex-map.git"
  },
  "keywords": [
    "яндекс карты",
    "яндекс-карты",
    "yandex maps",
    "yandex-maps",
    "maps",
    "vue"
  ],
  "author": "bizz.gc@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@nuxt/kit": "^3.8.0",
    "@yandex/ymaps3-types": "^0.0.17"
  },
  "devDependencies": {
    "@nuxt/types": "^2.17.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/jest": "^29.5.6",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-static-copy": "^0.17.0",
    "vue": "^3.3.5",
    "vue-demi": "^0.14.6",
    "vue-tsc": "^1.8.19"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "nuxt": "2||3",
    "vue": ">=2.7||3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    }
  }
}
